{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build dotnet-8-console-tests",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}/Csharp/Dotnet-8-Console-Tests/Dotnet-8-Console-Tests.csproj",
				"/property:GenerateFullPaths=true",
				"/consoleloggerparameters:NoSummary;ForceNoAlign"
			],
			"problemMatcher": "$msCompile"
		},
		{
			"label": "publish dotnet-8-console-tests",
			"command": "dotnet",
			"type": "process",
			"args": [
				"publish",
				"${workspaceFolder}/Csharp/Dotnet-8-Console-Tests/Dotnet-8-Console-Tests.csproj",
				"/property:GenerateFullPaths=true",
				"/consoleloggerparameters:NoSummary;ForceNoAlign"
			],
			"problemMatcher": "$msCompile"
		},
		{
			"label": "watch dotnet-8-console-tests",
			"command": "dotnet",
			"type": "process",
			"args": [
				"watch",
				"run",
				"--project",
				"${workspaceFolder}/Csharp/Dotnet-8-Console-Tests/Dotnet-8-Console-Tests.csproj"
			],
			"problemMatcher": "$msCompile"
		}
	]
}
